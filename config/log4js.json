{
  "appenders": [
    {
      "type": "console"
    },

    {
      "type": "clustered",
      "appenders": [
        {
          "type": "dateFile",
          "filename": "./log/wsdat.log",
          "pattern": "-yyyy-MM-dd",
          "category": "wsdat"
        },
        {
          "type": "dateFile",
          "filename": "./log/message.log",
          "pattern": "-yyyy-MM-dd",
          "category": "message"
        },
        {
          "type": "dateFile",
          "filename": "./log/httpaccess.log",
          "pattern": "-yyyy-MM-dd",
          "category": "http"
        },
        {
          "type": "dateFile",
          "filename": "./log/sqllog.log",
          "pattern": "-yyyy-MM-dd",
          "category": "sql"
        },

        {
          "type": "logLevelFilter",
          "level": "ERROR",
          "appender": {
            "type": "file",
            "filename": "./log/errors.log"
          }
        }
      ]
    }
  ]
}